<h2>Tasks</h2>

<% @account.tasks.each do |task| %>
  <%= task.name %>
  <%= task.requested %>
  <%= task.completed %><br>
<% end %>

<%= link_to 'Add a Task', new_account_task_path(@account) %>

<h2>Creative Developments</h2>

<% @account.creative_developments.each do |cd| %>
  <%= cd.design_name %>
  <%= cd.requested %>
  <%= cd.completed %><br>
<% end %>

<%= link_to 'Add a Creative Development', new_account_creative_development_path(@account) %>

